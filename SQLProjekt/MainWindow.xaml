<Window x:Class="SQLProjekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLProjekt"
        xmlns:uc="clr-namespace:SQLProjekt.Vievs"
        mc:Ignorable="d"
        Height="1000"
        Title="MainWindow" >
    <StackPanel Margin="20">
        <ContentControl Name="Test" Content="{Binding Screen}"></ContentControl>
        <Button Name="Return" Height="50" Width="100" Click="Button_Click" HorizontalAlignment="Right"/>
    </StackPanel>
</Window>
